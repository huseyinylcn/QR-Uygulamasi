<%- include('../Particals/header.ejs') %>

<div  id="b" class="pb-2 bg-dark" style="border-radius: 0; " >
    <%- include('../Particals/nav.ejs') %>
    
      
    
    </div>
<br>
    <div id="all" class="container">
        <div class="row">

            <% veri.forEach(function(item) { %>
                <div class="listeData col-12 col-xsm-12 col-md-6 col-xl-4 mt-2">
                    <div class="realdata">
                        <div class="left">
                            <h6><%= item.esertitle %></h6>
                            <p><%= item.city %> <%= item.town %> <%= item.adress %></p>
                           
                       
                        </div>
                        <a href="/<%= item.city %>/<%= item.town %>/<%= item.id %>">
                        <div class="right"><i class="bi bi-arrow-right-square-fill"></i></div>
                    </a>
                    </div>
                </div>
               
            <% }); %>
        


        </div>




    </div>
    <br><br><br><br><br>
    <%- include('../Particals/footer.ejs') %>